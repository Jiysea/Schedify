                    </path>
                </svg>
            </span>
            <div class="flex flex-col justify-start grow">
                <span class="font-semibold drop-shadow-sm text-coffee-900">
                    @Model.GlobalTotalEventsDrafted
                </span>
                <h2 class="text-sm drop-shadow-sm">Draft Events</h2>
            </div>
        </div>

        @* Total Opened Events *@
        <div class="flex items-center gap-2 mx-2 my-2  p-2 rounded-lg shadow bg-coffee-50 text-coffee-600">
